{
	"name": "eggbe/struct",
	"version": "0.1.1",
	"type": "library",
	"keywords": ["structures", "struct", "php-struct", "structures emulation", "structure-like"],
	"description": "Struct features emulation for php.",
	"license": "MIT",
	"authors": [
		{
			"name": "hacpaka",
			"email": "hacpaka@eggbe.com"
		}
	],
	"require": {
		"php": ">=7.1.0",
		"eggbe/helpers": "dev-master",
		"eggbe/prototype": "dev-master"
	},
	"require-dev": {
		"phpunit/phpunit": "^7"
	},
	"autoload": {
		"psr-4": {
			"Eggbe\\Struct\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Eggbe\\Struct\\Tests\\": "tests/"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
